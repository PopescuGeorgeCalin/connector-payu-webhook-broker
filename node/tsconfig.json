{
  "compilerOptions": {
    "types": [
      "bluebird-global",
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "target": "es2019",
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2016",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "outDir": "service",
    "allowJs": true,
    "inlineSourceMap": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "diagnostics": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "./typings.d.ts",
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
